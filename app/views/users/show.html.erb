<div class="row" >
  <div class="col-md-6 offset-md-3" >
    <nav class="nav nav-pills nav-justified">
      <li class="nav-item">
        <%= link_to "Posts", user_path(@user.username), class: "nav-link" %> 
      </li>
      <li class="nav-item">
        <%= link_to "Likes", liked_photos_path(@user.username), class: "nav-link" %> 
      </li>
      <li class="nav-item">
        <%= link_to "Followers", user_path(@user.username), class: "nav-link" %> 
      </li>
      <li class="nav-item">
        <%= link_to "Following", user_path(@user.username), class: "nav-link" %> 
      </li>
      <li class="nav-item">
        <%= link_to "Feed", user_path(@user.username), class: "nav-link" %> 
      </li>
    </nav>
  </div>
</div>
<h1>
<%= @user.username %>
</h1>

<%= current_user.own_photos.count %>
<% current_user.own_photos.each do |photo| %>
  <%= render "photos/photo", photo: photo %>
<%end%>
